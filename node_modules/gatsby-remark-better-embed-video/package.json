{
	"name": "gatsby-remark-better-embed-video",
	"version": "2.4.0",
	"description": "Embed Videos (Youtube, Vimeo, VideoPress) in Gatsby via Markdown with better options",
	"main": "index.js",
	"author": "Ahmad Awais <https://twitter.com/MrAhmadAwais/>",
	"license": "MIT",
	"keywords": [
		"gatsby",
		"gatsby-plugin",
		"remark",
		"markdown",
		"gatsby-remark-better-embed-video",
		"gatsby-remark-embed-video"
	],
	"dependencies": {
		"@types/node": "^10.3.3",
		"get-video-id": "^3.1.0",
		"unist-util-visit": "^1.3.1"
	},
	"devDependencies": {
		"auto-changelog": "^1.8.0",
		"cross-env": "^5.2.0",
		"release-it": "^7.6.1",
		"typescript": "^2.9.2"
	},
	"homepage": "https://github.com/ahmadawais/gatsby-remark-better-embed-video#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ahmadawais/gatsby-remark-better-embed-video.git"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": ".clTemplate.hbs",
		"commitLimit": false,
		"package": true,
		"replaceText": {
			"FIX: ": "",
			"NEW: ": "",
			"IMPROVE: ": "",
			"DOC: ": ""
		}
	},
	"scripts": {
		"build": "tsc",
		"prepublish": "cross-env NODE_ENV=production npm run build",
		"watch": "tsc -w",
		"log": "auto-changelog && git add . && git ci -m ' ðŸ“– DOC: Update the changelog' && git push",
		"release": "release-it",
		"release-minor": "release-it minor",
		"release-major": "release-it major",
		"release-dry": "release-it --dry-run",
		"release-beta": "release-it --preRelease=beta"
	}
}
